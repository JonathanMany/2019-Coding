adj1 = raw_input("Tell me an Adjective and click enter!")
noun1 = raw_input("Tell me an Noun and click enter!")
noun2 = raw_input("Tell me another noun and Click Enter!")
adj2 = raw_input("Tell me one last adjective!")

print "Roses are " + adj1
print noun1 + " are blue."
print noun1 + " is " + adj2
print "And so are you!"
